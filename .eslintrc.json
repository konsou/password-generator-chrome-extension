{
  "env": {
    "browser": true,
    "es2021": true,
    "webextensions": true
  },
  "extends": "airbnb-base",
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-use-before-define": [
      "error",
      {
        "functions": false
      }
    ]
  }
}
